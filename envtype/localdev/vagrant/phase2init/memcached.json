{
  "id": "memcached",
  "cpus": 0.3,
  "mem": 140.0,
  "instances": 2,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "memcached",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 11211,
          "hostPort": 0,
          "protocol": "tcp",
          "servicePort": 5000
        }
       ]
    }
  },
  "upgradeStrategy": {
      "minimumHealthCapacity": 0
  }

}
